{% assign path = vite-tag | replace: '~/', '../' | replace: '@/', '../' %}
{% if path == "/frontend/entrypoints/chatbot-core-file.js" or path == "chatbot-core-file.js" %}
  <script src="{{ 'vite-chatbot-core.js' | asset_url | split: '?' | first }}" type="module" crossorigin="anonymous"></script>
{% elsif path == "/frontend/entrypoints/chatbot-main.js" or path == "chatbot-main.js" %}
  <script src="{{ 'vite-chatbot-main.js' | asset_url | split: '?' | first }}" type="module" crossorigin="anonymous"></script>
  <link rel="modulepreload" href="{{ 'vite-chatbot-core.js' | asset_url | split: '?' | first }}" crossorigin="anonymous">
{% endif %}
